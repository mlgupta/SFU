    <CreateCommand>useradd</CreateCommand>
    <ModifyCommand>usermod</ModifyCommand>
    <DeleteCommand>userdel</DeleteCommand>
    <CryptCommand>openssl</CryptCommand>
    <CryptParam>passwd</CryptParam>
    <CommentParam>-c</CommentParam>
    <PasswordParam>-p</PasswordParam>
    <PasswordFile>/etc/passwd</PasswordFile>
